[Unit]
Description=SockAct Example UNIX domain socket
After=local-fs.target
Requires=local-fs.target

[Socket]
ListenStream=/run/uds-daemon.sock
SocketMode=0660
SocketUser=root
# Current user should be member of users
SocketGroup=users

# Clean up socket on stop
RemoveOnStop=yes

[Install]
WantedBy=sockets.target
