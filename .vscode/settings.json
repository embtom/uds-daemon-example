{
    "cmake.ctest.debugConfig": "Debug CTest",
    "cmake.useCMakePresets": "always",
    "cmake.options.statusBarVisibility": "visible",
    "cmake.sourceDirectory": "${workspaceFolder}/uds-daemon-example",
    "cmake.configureOnOpen": false,
    "cmake.configureOnEdit": false,
    "cmake.loadCompileCommands": false,
    "cmake.skipConfigureIfCachePresent": true,
    "cmake.parallelJobs": 0,
    "cmake.copyCompileCommands": "${workspaceFolder}/uds-daemon-example/build/compile_commands.json",
    "sonarlint.pathToCompileCommands": "${workspaceFolder}/uds-daemon-example/build/compile_commands.json",
    "shellformat.path": "/usr/bin/shfmt",
    "[markdown]": {
        "editor.defaultFormatter": "yzhang.markdown-all-in-one"
    },
    "[shellscript]": {
        "editor.defaultFormatter": "foxundermoon.shell-format"
    },
    "cmakeFormat.args": [
        "--config-file",
        "${workspaceFolder}/cmake-format.json"
    ],
    "files.associations": {
        "array": "cpp",
        "atomic": "cpp",
        "bit": "cpp",
        "cctype": "cpp",
        "charconv": "cpp",
        "chrono": "cpp",
